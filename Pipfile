[[source]]
name = "pypi"
url = "https://pypi.org/simple"
verify_ssl = true

[dev-packages]
pytest = "*"

[packages]

[requires]
python_version = "3.9"

[scripts]
unittest = "pipenv run pytest -s"
unittest-log = "pipenv run pytest -s --log-cli-level=20"
unittest-all-log = "pipenv run pytest -s --log-cli-level=10"